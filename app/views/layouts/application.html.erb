<!DOCTYPE html>
<html>
<head>
  <title>Videos</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
<% if alert %>
<div class="alert alert-error"> <%= alert %>
<% end %>
<% if notice %>
<div class="alert alert-success"> <%= notice %>
</div>
</div>
<% end %>
<% if user_signed_in? %> <li><%= link_to 'Logout', destroy_user_session_path, method: :delete %></li> <% end %>
<%= yield %>
<ul class="nav">
               <li class="active"><a href="/">Home</a></li>
               <li><%= link_to "Tracks", tracks_path %></li>
              <li><%= link_to "Videos", videos_path %></li>
              <li><%= link_to "Statuses", statuses_path %></li>
              <li><%= link_to "Kinds", kinds_path %></li>
             </ul>
</body>
</html>
